    "@angular/material": "^5.2.5",
    "@angular/platform-browser": "5.2.0",
    "@angular/platform-browser-dynamic": "5.2.0",
    "@angular/router": "5.2.0",
    "@ngtools/webpack": "^1.9.3",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "9.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
