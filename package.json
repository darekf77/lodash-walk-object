{
    "name": "lodash-walk-object",
    "description": "Walk all properties deep in object with lodash help",
    "version": "0.0.1",
    "tnp": {
        "type": "isomorphic-lib",
        "isCommandLineToolOnly": true,
        "overrided": {
            "includeAsDev": [
                "morphi",
                "@angular.*",
                "webpack",
                "webpack-dev-server",
                "html-webpack-plugin",
                "ts-loader",
                "babel.*",
                "mocha",
                "@types/mocha",
                "chai",
                "ts-sinon"
            ],
            "includeOnly": [
                "lodash",
                "@types/lodash"
            ]
        }
    },
    "scripts": {
        "test": "mocha --require ts-node/register src/**/*.spec.ts",
        "test:watch": "mocha --compilers ts-node/register src/**/*.spec.ts --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/darekf77/lodash-walk-object.git"
    },
    "keywords": [
        "ng2-rest",
        "docs",
        "documentation"
    ],
    "author": "Dariusz Filipiak",
    "bugs": {
        "url": "https://github.com/darekf77/lodash-walk-object/issues"
    },
    "homepage": "https://github.com/darekf77/lodash-walk-object#readme"
}