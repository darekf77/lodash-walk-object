{
  "extends": "./tsconfig.isomorphic.json",
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strictNullChecks": false,
    "rootDir": "./",
    "esModuleInterop": true
  },
  "include": [
    "src/**/*",
    "environments/**/*.ts",
    "env.ts"
  ],
  "exclude": [
    "src/assets"
  ]
}
